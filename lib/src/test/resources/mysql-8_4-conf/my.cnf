[mysqld]
# MySQL 8.4 では --skip-host-cache が削除されているため、代替の host_cache_size=0 を指定
host_cache_size=0
# 名前解決を無効化（起動高速化・安定化）
skip-name-resolve
# 文字コードと照合順序
character-set-server=utf8mb4
collation-server=utf8mb4_unicode_ci
# InnoDB 初期化の安定化に資する軽微な設定（必要に応じて調整可）
innodb_flush_log_at_trx_commit=1
innodb_buffer_pool_size=134217728
